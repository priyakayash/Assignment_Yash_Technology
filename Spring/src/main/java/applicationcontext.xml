<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="com.yash.spring_assignment.Spring.Emp" name="trainee">
		<property name="empId" value="y001"/>
		<property name="empName" value="yash"/>
		<property name="empRole" value="trainee"/>
	</bean>
	<bean class="com.yash.spring_assignment.Spring.PrintMessage" name="print">
		<property name="Message" value="yash"/>
	</bean>
	
	<bean class="com.yash.spring_assignment.Spring.ConstructorMessage" name="message">	
	<constructor-arg value="yash technology"/>
	
	
	
	</bean> 
	
	<bean class="com.yash.spring_assignment.Spring.SetterMessage" name="settermessage">
			<property name="Message" value="null"/>

	</bean>
	
</beans>
